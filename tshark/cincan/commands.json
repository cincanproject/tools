{
  "commands": [
    {
      "command": ["-r", "<file>", "-Tjson"],
      "input": "application/vnd.tcpdump.pcap",
      "output": "application/json"
    }, {
      "command": ["-r", "<file>", "-Tpdml"],
      "input": "application/vnd.tcpdump.pcap",
      "output": "text/xml"
    }
  ],
 "output-options": [
    {
      "stdout": true
    },
    {
      "args": ["-w", "<out-file>"]
    }
  ]
}
